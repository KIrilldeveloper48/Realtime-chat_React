{
  "tags": {
      "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]

  },
  "source": {
      "include": ["./src"]
      
  },
  "plugins": [
      "plugins/markdown",
      "better-docs/component"
  ],
  "opts": {
      "encoding": "utf8",
      "destination": "docs/",
      "recurse": true,
      "verbose": true,
      "template": "./node_modules/better-docs"
  },
  "templates": {
      "better-docs": {
          "name": "Документация Realtime chat"
      }
  }
}